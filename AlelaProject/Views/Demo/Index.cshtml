<script type="text/javascript">

    function functionConfirm(event) {

        var idObjetod = document.getElementById('idObjeto').selectedOptions[0].value;
        var Fecha = $("#Fecha").val();
        var HoraInicio = document.getElementById('HoraInicio').selectedOptions[0].value;
        var HoraFin = document.getElementById('Horaf').selectedOptions[0].value;

        if (Fecha == "" || idObjetod == "" || HoraInicio == "" || HoraFin=="") {
            swal({
                title: 'Error',
                text: "Todos los campos son obligatorios",
                type: 'error',
                showCancelButton: false,
                cancelButtonText: 'No',
                cancelButtonClass: 'btn btn-danger',
                showConfirmButton: true,
                confirmButtonText: 'Aceptar',
                confirmButtonClass: 'btn btn-success'
            }, function (isConfirm) {
                if (isConfirm) {
                    return true;
                } else {
                    return false;
                }
            });
        }
        else {
            swal({
                title: 'Confirmación',
                text: "¿ Esta seguro que desea actualizar la información ?",
                type: 'warning',
                showCancelButton: true,
                cancelButtonText: 'No',
                cancelButtonClass: 'btn btn-danger',
                showConfirmButton: true,
                confirmButtonText: 'Si',
                confirmButtonClass: 'btn btn-success'
            }, function (isConfirm) {
                if (isConfirm) {
                    location.href = '@Url.Action("IndexPost", "Demo")?idObjeto=' + idObjetod + '&Fecha=' + Fecha + '&HoraInicio=' + HoraInicio + '&HoraFin=' + HoraFin;
                    return true;
                } else {
                    return false;
                }
            });
        }

        return false;
    }

</script>

