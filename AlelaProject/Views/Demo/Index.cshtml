
@{ ViewBag.Title = "Inicio";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
    List<SelectListItem> Area = (List<SelectListItem>)ViewBag.Data;
    List<SelectListItem> HoraInical = (List<SelectListItem>)ViewBag.DataH;
    List<SelectListItem> Horaf = (List<SelectListItem>)ViewBag.DataF;
}

@section styles{
    <style></style>
}

<br />
<div class="alert-danger">
    @ViewBag.Error
</div>

<br />
<br />
<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <div class="modal" id="reservasModal" tabindex="-1">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-content">
                            <div class="modal-header">

                                <span style="font-size:16px;color:cadetblue;font-family:'Arial Rounded MT'">Nueva Reserva</span>
                                <button class="close" data-dismiss="modal">&times;</button>

                            </div>
                            <div class="modal-body" style="font-weight:bold">
                                <form class="form-group" asp-action="Index" id="formulario">
                                    <div class="form-group">
                                        <label style="font-family:'Arial Rounded MT';font-weight:bold;font-size:15px">Seleccione el Puesto</label>
                                        @Html.DropDownList("idObjeto", Area, "Selecciona Puesto", new { @class = "form-control" })
                                    </div>
                                    <div class="form-group">
                                        <label style="font-family:'Arial Rounded MT';font-weight:bold;font-size:15px">Fecha</label>
                                        <input id="Fecha" name="Fecha" class="form-control" type="date" style="width:100%" />
                                    </div>
                                    <div class="form-group">
                                        <label style="font-family:'Arial Rounded MT';font-weight:bold;font-size:15px">Hora Entrada</label>
                                        @Html.DropDownList("HoraInicio", HoraInical, "Selecciona Hora Inicial", new { @class = "form-control" })
                                    </div>
                                    <div class="form-group">
                                        <label style="font-family:'Arial Rounded MT';font-weight:bold;font-size:15px">Hora Salida</label>
                                        @Html.DropDownList("Horaf", Horaf, "Selecciona Hora Salida", new { @class = "form-control" })
                                    </div>
                                </form>
                            </div>
                            <div class="modal-footer">
                                <button class="btn btn-outline-info" type="button" style="width:150px" onclick="functionConfirm();">Aceptar</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
