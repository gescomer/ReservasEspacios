<script type="text/javascript">
    function mostrarResumen(codigo) {

        var v  = '@ViewBag.DatosMapa';

            var array = @Html.Raw(Json.Encode(
            ((IEnumerable<AlelaProject.Models.Mapa>)ViewBag.DatosMapa).Select(Mapa => new
            {
                idObjeto = Mapa.idObjeto,
                Nombre = Mapa.Nombre,
                Codigo = Mapa.Codigo,
                NombreArea = Mapa.NombreArea,
                TipoObjeto = Mapa.TipoObjeto,
                idArea = Mapa.idArea,
                NombreComplemento = Mapa.NombreComplemento,
            }) ) );
            for (var i = 0; i < array.length; i += 1) {

                if (array[i].idObjeto == codigo) {
                    $("#modal_idd").html(array[i].idObjeto);
                    $("#modal_id2").html(array[i].idObjeto);
                    $("#modal_idh").html(array[i].idObjeto);
                    $("#modal_cod").html(array[i].Codigo);
                    $("#modal_cod2").html(array[i].Codigo);
                    $("#modal_nombre").html(array[i].Nombre + ' / ' + array[i].Codigo)
                    $("#modal_nombre2").html(array[i].Nombre + ' / ' + array[i].Codigo);
                    $("#modal_tipo").html(array[i].TipoObjeto);
                    $("#modal_tipo2").html(array[i].TipoObjeto);
                    $("#modal_area").html(array[i].NombreArea);
                    $("#modal_area2").html(array[i].NombreArea);
                }
            }

        var arrayComple = @Html.Raw(Json.Encode(
            ((IEnumerable<AlelaProject.Models.ObjetosComplementos>)ViewBag.DatosMapaCompl).Select(Mapa => new
            {
                idObjeto = Mapa.idObjeto,
                Complementos = Mapa.Complementos
            })));

            for (var i = 0; i < arrayComple.length; i += 1) {

                if (arrayComple[i].idObjeto == codigo) {
                    $("#modal_complemento").html(arrayComple[i].Complementos);
                    $("#modal_complemento2").html(arrayComple[i].Complementos);
                }
            }

            var arrayReser = @Html.Raw(Json.Encode(
            ((IEnumerable<AlelaProject.Models.ConsultaReserva>)ViewBag.DatosReservas).Select(Mapa => new
            {
                Fecha = Mapa.Fecha,
                idObjeto = Mapa.idObjeto,
                HoraInicio = Mapa.HoraInicio,
                HoraFin = Mapa.HoraFin,
                NombreCompleto = Mapa.NombreCompleto,
                FechaReserva = Mapa.FechaReserva,
                Disponible = Mapa.Disponible,
                NombreHora = Mapa.NombreHora,
                observaciones = Mapa.Observaciones,
                nombredia = Mapa.NombreDia,
            })));

            var ingreso = '1';
            $("#modal_nombrec").html('');
            $("#modal_fecha").html('');
            $("#modal_horai").html('');
            $("#modal_horaf").html('');
            document.getElementById("datosPlano1").innerHTML = "";

            for (var i = 0; i < arrayReser.length; i += 1) {

                if (arrayReser[i].idObjeto == codigo) {
                    if (arrayReser[i].Disponible == "Pendiente" || arrayReser[i].Disponible == "Hoy") {

                        if (ingreso == '1') {
                            $("#modal_nombrec").html(arrayReser[i].NombreCompleto);
                            $("#modal_fecha").html(arrayReser[i].FechaReserva);
                            $("#modal_horai").html(arrayReser[i].HoraInicio);
                            $("#modal_horaf").html(arrayReser[i].HoraFin);
                            ingreso = '0';
                        }

                        $("#datosPlano1").append(
                            "<tr style='text-align:center'><td style='font-weight:bold'>"
                            + arrayReser[i].FechaReserva + "-" + "<span class='badge bg-warning'>" + arrayReser[i].nombredia + "</span></td><td>"
                            + arrayReser[i].HoraInicio + "</td><td>"
                            + arrayReser[i].HoraFin + "</td>" +
                            "<td>" + arrayReser[i].NombreCompleto + "</td><td>"
                            + arrayReser[i].Disponible + "</td><td>"
                            + arrayReser[i].NombreHora + "</td><td>"
                            + arrayReser[i].observaciones + "</td></tr>"
                        );
                    //    otras = otras + arrayReser[i].FechaReserva + ' ' + arrayReser[i].HoraInicio + ' ' + arrayReser[i].HoraFin + ' ' + arrayReser[i].NombreCompleto + ' - ' + arrayReser[i].Disponible + ' - Hora Almuerzo:' + arrayReser[i].NombreHora + ' - '+ arrayReser[i].observaciones + '<br/>';
                    }
                }
            }

        if (codigo == "1221" || codigo == "1222") {
            $('#resumenModalAuditorio').modal('show');
        }
        else {
            $('#resumenModal').modal('show');
        }


    }
</script>

