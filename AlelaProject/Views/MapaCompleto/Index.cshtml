<script>
    var establecerHorario = function () {
        var fechainicial = $("#fechainicial").val();
        var fechafin = $("#fechafin").val();
        var horainicio = document.getElementById('horainicio').selectedOptions[0].value;
        var horafin = document.getElementById('horafin').selectedOptions[0].value;
        var idalmuerzo = document.getElementById('idalmuerzo').selectedOptions[0].value;
        var puestos = $("#modal_idh").text();
        var usuario = '@ViewBag.IdUser';

        var arregloitems = [];

        var coma = "";

        $("#itemNov td input[type=checkbox]").each(function (index, val) {

            var current = $(val).attr("Id");

            var isChecked = $("#" + current).is(":checked", true);

            if (isChecked) {
                arregloitems.push(current);
            }
        })

        if (fechainicial == "" || fechafin == "" || horainicio == "" || horafin == "" ||
            puestos == "" || usuario == "" || idalmuerzo == "") {
            swal({
                title: 'Error',
                text: "Todos los campos son obligatorios",
                type: 'error',
                showCancelButton: false,
                cancelButtonText: 'No',
                cancelButtonClass: 'btn btn-danger',
                showConfirmButton: true,
                confirmButtonText: 'Aceptar',
                confirmButtonClass: 'btn btn-success'
            }, function (isConfirm) {
                if (isConfirm) {
                    return true;
                } else {
                    return false;
                }
            });
        }
        else {
            swal({
                title: 'Confirmación',
                text: "¿ Esta seguro que desea ingresar reservas desde " + fechainicial + " hasta " + fechafin + "?",
                type: 'warning',
                showCancelButton: true,
                cancelButtonText: 'No',
                cancelButtonClass: 'btn btn-danger',
                showConfirmButton: true,
                confirmButtonText: 'Si',
                confirmButtonClass: 'btn btn-success'
            }, function (isConfirm) {
                if (isConfirm) {
                    location.href = '@Url.Action("IndexAceptarmuliple", "Horario")?ids=' + arregloitems + '&fechainicial=' + fechainicial + '&fechafin=' + fechafin + '&horainicio=' + horainicio + '&horafin=' + horafin + '&Puesto=' + puestos + '&usuarios=' + usuario + '&Almuerzo=' + idalmuerzo;
                    return true;
                } else {
                    return false;
                }
            });
        }
    }
</script>

