<script>
    function functionreservarid(event) {
        var user = '@ViewBag.IdUser';
        var email = '@ViewBag.CorreoElectronico';
        var Fecha = $("#Fechar").val();
        var HoraInicio = document.getElementById('HoraInicior').selectedOptions[0].value;
        var HoraFin = document.getElementById('Horafr').selectedOptions[0].value;

        var Fecha2 = $("#Fechar2").val();
        var HoraInicio2 = document.getElementById('HoraInicior2').selectedOptions[0].value;
        var HoraFin2 = document.getElementById('Horafr2').selectedOptions[0].value;

        var idObjetod = $("#modal_id").text();
        var idObjetod2 = $("#modal_id2").text();

        var Almuerzo = document.getElementById('Almuerzo').selectedOptions[0].value;
        var observaciones = $("#observaciones").val();
        var contador = 0;
        var nombrepuesto = $("#modal_nombre").text();

        if (idObjetod == "") {

            idObjetod = idObjetod2;
        }

        if (observaciones == "") {
            observaciones = "";
        }

        if (Fecha == "") {

            Fecha = Fecha2;
        }
        if (HoraInicio == "") {

            HoraInicio = HoraInicio2;
        }
        if (HoraFin == "") {

            HoraFin = HoraFin2;
        }



        /*buscamos el aforo maximo para el dia seleccinado*/

        var array = @Html.Raw(Json.Encode(
            ((IEnumerable<AlelaProject.Models.HoraAlmuerzo>)ViewBag.almuerzoreservado).Select(Mapa => new
            {
                idHoraAlmuerzo = Mapa.idHoraAlmuerzo,
                Fecha = Mapa.Fecha.ToString(),
            }) ) );

        for (var i = 0; i < array.length; i += 1)
        {

            if (array[i].Fecha == Fecha && array[i].idHoraAlmuerzo == Almuerzo)
            {
              contador = contador + 1;
            }
        }

        if (idObjetod == "1221" || idObjetod == "1222") {

            Almuerzo = "1002";
        }

        if (contador > 6) {
            swal({
                title: 'Error',
                text: "Aforo maximo para la hora de almuerzo seleccionado.",
                type: 'error',
                showCancelButton: false,
                cancelButtonText: 'No',
                cancelButtonClass: 'btn btn-danger',
                showConfirmButton: true,
                confirmButtonText: 'Aceptar',
                confirmButtonClass: 'btn btn-success'
            }, function (isConfirm) {
                if (isConfirm) {
                    return true;
                } else {
                    return false;
                }
            });
        }
        else {

            if (Fecha == "" || (idObjetod == "") || HoraInicio == "" || HoraFin == "" || Almuerzo == "") {
            swal({
                title: 'Error',
                text: "Todos los campos son obligatorios",
                type: 'error',
                showCancelButton: false,
                cancelButtonText: 'No',
                cancelButtonClass: 'btn btn-danger',
                showConfirmButton: true,
                confirmButtonText: 'Aceptar',
                confirmButtonClass: 'btn btn-success'
            }, function (isConfirm) {
                if (isConfirm) {
                    return true;
                } else {
                    return false;
                }
            });
        }
            else {
            swal({
                title: 'Confirmación',
                text: "¿ Esta seguro que desea reservar para el  " + Fecha +" ?",
                type: 'warning',
                showCancelButton: true,
                cancelButtonText: 'No',
                cancelButtonClass: 'btn btn-danger',
                showConfirmButton: true,
                confirmButtonText: 'Si',
                confirmButtonClass: 'btn btn-success'
            }, function (isConfirm) {
                if (isConfirm) {
                    location.href = '@Url.Action("IndexPost", "MapaCompleto")?idObjeto=' + idObjetod + '&Fecha=' + Fecha + '&HoraInicio=' + HoraInicio + '&HoraFin=' + HoraFin + '&iduser=' + user + '&Almuerzo=' + Almuerzo + '&observaciones=' + observaciones + '&nombrepuesto=' + nombrepuesto+ '&Email=' + email;
                    return true;
                } else {
                    return false;
                }
            });
        }
        }

        return false;
    }
</script>

